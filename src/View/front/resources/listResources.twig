{% extends 'layout/layout.twig' %}

{% block title %}Asperger Resources{% endblock %}

{% block summary %}}Resources list about Asperger Syndrome{% endblock %}

{% block main %}
    <main>
        <ul class="row">
            {% for category in allResources|keys %}
            <li>
                <a class="btn-blue" href="{{ url('resources!read', {'category': category}) }}">
                    <h3 class="mar-none">{{ category }}</h3>
                </a>
            </li>
            {% endfor %}
        </ul>
    </main>
{% endblock %}
